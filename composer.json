{
    "name": "osa-eg/laravel-teams-notification",
    "description": "A Laravel package to send notifications to Microsoft Teams",
    "keywords": ["notification", "teams", "microsoft-teams-workflow", "logging","teams_logging","teams-workflow","teams-connector", "laravel", "adaptive-card","teams-webhock"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "guzzlehttp/guzzle": ">=6.5",
        "monolog/monolog": ">=1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Osama\\LaravelTeamsNotification\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Osama\\LaravelTeamsNotification\\LaravelTeamsNotificationServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Osama Saad",
            "email": "osaad96eg@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
